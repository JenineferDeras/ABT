{
  "git.autofetch": false,
  "git.autoRefresh": false,
  "git.path": "/usr/bin/git",
  "git.enabled": true,
  "git.autoRepositoryDetection": true,
  "git.detectSubmodules": false,
  "git.ignoreLimitWarning": true,
  "json.schemaDownload.enable": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.preferences.npmLocation": "/usr/local/bin/npm",
  "typescript.npm": "/usr/local/bin/npm",
  "typescript.tsc.autoDetect": "on",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "npm.packageManager": "npm",
  "npm.executable": "/usr/local/bin/npm",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/bin/bash"
    }
  },
  "redhat.telemetry.enabled": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.rulers": [80, 120],
  "editor.wordWrap": "on",
  "editor.minimap.enabled": true,
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "files.associations": {
    "*.tsx": "typescriptreact",
    "*.ts": "typescript",
    "*.json": "jsonc"
  },
  "emmet.includeLanguages": {
    "typescript": "html",
    "typescriptreact": "html"
  },
  "tailwindCSS.includeLanguages": {
    "typescript": "html",
    "typescriptreact": "html"
  },
  "tailwindCSS.experimental.classRegex": ["cn\\(([^)]*)\\)"],
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  "python.terminal.activateEnvironment": false,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.next": true,
    "**/dist": true,
    "**/__pycache__": true,
    "**/*.pyc": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.next": true,
    "**/dist": true,
    "**/__pycache__": true
  },
  "extensions.ignoreRecommendations": false,
  "workbench.startupEditor": "welcomePage",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "security.workspace.trust.untrustedFiles": "open",
  "debug.console.wordWrap": false,
  "debug.terminal.clearBeforeReusing": true,
  "eslint.workingDirectories": ["./"],
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "nextjs.serverPort": 3000,
  "sonarlint.connectedMode.project": {
    "connectionId": "jeninefer",
    "projectKey": "Jeninefer_nextjs-with-supabase"
  },
  "sonarlint.analysisExcludesStandalone": "**/lib/**,**/*generated*",
  "sonarlint.automaticAnalysis": true,
  "sonarlint.focusOnNewCode": false,
  "sonarlint.disableTelemetry": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "jsonc": true,
    "typescript": true,
    "typescriptreact": true,
    "javascript": true,
    "javascriptreact": true
  },
  "github.copilot.chat.localeOverride": "en",
  "http.proxyStrictSSL": false,
  "http.systemCertificates": false
}
