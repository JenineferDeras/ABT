{
  "git.autofetch": false,
  "git.autoRefresh": false,
  "git.path": "/opt/homebrew/bin/git",
  "git.enabled": true,
  "git.autoRepositoryDetection": true,
  "git.detectSubmodules": false,
  "json.schemaDownload.enable": true,
