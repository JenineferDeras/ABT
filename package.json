{
  "name": "nextjs-with-supabase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "bash scripts/start-dev.sh",
    "dev:stop": "bash scripts/stop-dev.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next out node_modules/.cache",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:coverage": "jest --config jest.config.ts --coverage",
    "test:ci": "jest --config jest.config.ts --ci --coverage --maxWorkers=2",
    "dashboard": "source abaco_venv/bin/activate && streamlit run notebooks/ml_dashboard.py",
    "dashboard:prod": "source abaco_venv/bin/activate && streamlit run notebooks/ml_dashboard.py --server.port 8501 --server.address 0.0.0.0",
    "ports:clean": "bash scripts/cleanup-ports.sh",
    "supabase:migrate": "supabase db push",
    "supabase:reset": "supabase db reset"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "^2.39.0",
    "applicationinsights": "^3.12.0",
    "class-variance-authority": "latest",
    "clsx": "latest",
    "lucide-react": "latest",
    "next": "^15.0.0",
    "next-themes": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "latest",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "latest",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
