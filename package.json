    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "validate": "npm run type-check && npm run lint",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
